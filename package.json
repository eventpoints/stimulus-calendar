{
  "name": "stimulus-clock",
  "version": "1.0.1",
  "description": "A Stimulus controller to allow date and time input.",
  "keywords": [
    "stimulus",
    "stimulusjs",
    "stimulus controller",
    "clock",
    "datetime"
  ],
  "author": "Kerrial Newham <kerrialbeckettnewham@gmail.com>",
  "license": "MIT",
  "private": false,
  "main": "dist/stimulus-popover.umd.js",
  "module": "dist/stimulus-popover.es.js",
  "scripts": {
    "format": "prettier-standard  '**/*.{ts,css,html}' --format",
    "lint": "prettier-standard  '**/*.{ts,css,html}' --lint",
    "dev": "vite",
    "prod": "vite build --mode netlify",
    "build": "tsc --noEmit && vite build",
    "version": "yarn build",
    "np": "np --no-2fa --no-test"
  },
  "devDependencies": {
    "@babel/core": "7.18.10",
    "@babel/plugin-syntax-class-properties": "7.12.13",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@hotwired/stimulus": "^3.2.1",
    "np": "^7.6.2",
    "prettier-standard": "16.4.1",
    "typescript": "^4.7.4",
    "vite": "^2.3.6"
  },
  "peerDependencies": {
    "@hotwired/stimulus": "^3.1.0"
  },
  "dependencies": {
    "moment": "^2.29.4",
    "stimulus-use": "^0.51.1"
  }
}
